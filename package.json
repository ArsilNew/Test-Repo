    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.19",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
